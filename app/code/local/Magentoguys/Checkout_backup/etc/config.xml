<?xml version="1.0"?>
<config>
    <modules>
        <Magentoguys_Checkout>
            <version>1.0.1</version>
        </Magentoguys_Checkout>
    </modules>
    <frontend>
		<routers>
			<checkout>
				<args>
					<modules>
						<magentoguys_checkout before="Mage_Checkout">Magentoguys_Checkout</magentoguys_checkout>
					</modules>
				</args>
			</checkout>
		</routers>
	</frontend>
	<global>
		<blocks>
			<checkout>
				<rewrite>
					<onepage_billing>Magentoguys_Checkout_Block_Onepage_Billing</onepage_billing>
				</rewrite>
			</checkout>
		</blocks>
		<models>
			<sales>
				<rewrite>
					<service_quote>Magentoguys_Checkout_Model_Sales_Service_Quote</service_quote>
				</rewrite>
			</sales>
			<sales>
				<rewrite>
					<order>Magentoguys_Checkout_Model_Sales_Order</order>
				</rewrite>
			</sales>
		</models>
	</global>
</config>
